{
  "name": "solodevflow",
  "version": "1.0.0",
  "description": "AI超级个体开发助手 - 从需求到部署的完整人机协作开发闭环",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "solodev": "./src/command-system/cli.ts"
  },
  "scripts": {
    "build": "tsc",
    "solodev": "npx tsx src/command-system/cli.ts",
    "validate:state": "npx tsx src/validators/cli.ts state",
    "validate:refs": "npx tsx src/validators/cli.ts refs",
    "validate:all": "npx tsx src/validators/cli.ts all",
    "context:phase": "npx tsx src/context/context-loader.ts phase",
    "context:module": "npx tsx src/context/context-loader.ts module"
  },
  "keywords": ["ai", "development", "assistant", "solo-developer"],
  "author": "SoloDevFlow",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
